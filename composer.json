{
    "name": "ttpn18121996/simple-repository",
    "description": "Build repository and service patterns quickly and simply.",
    "keywords": ["repository", "simple-repository", "service", "simple-service"],
    "license": "MIT",
    "authors": [
        {
            "name": "Trinh Tran Phuong Nam",
            "email": "ttpn18121996@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "illuminate/collections": "^12.0",
        "illuminate/console": "^12.0",
        "illuminate/contracts": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/support": "^12.0",
        "illuminate/log": "^12.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "SimpleRepository\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleRepository\\SimpleRepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
