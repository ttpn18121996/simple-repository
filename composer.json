{
    "name": "ttpn18121996/simple-repository",
    "description": "Build a quick and simple repository pattern.",
    "keywords": ["repository", "simple-repository"],
    "license": "MIT",
    "authors": [
        {
            "name": "Trinh Tran Phuong Nam",
            "email": "ttpn18121996@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/console": "^9.21|^10.0",
        "illuminate/contracts": "^9.21|^10.0",
        "illuminate/database": "^9.21|^10.0",
        "illuminate/support": "^9.21|^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "dev-master",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SimpleRepository\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimpleRepository\\SimpleRepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
